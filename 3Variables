#Defining an INT
#---------------
a =27
print(a)
print("value of a {}".format(a))


#Defining FLOAT
#-------------
b=27.0
print(b)
print("value of b {}".format(b))

#Converting INT to FLOAT
#-----------------------
c=27
print(float(c))
print("value of c {}".format(c)) #Note it's int only :)

#Defining  String
#----------------
## a=sarab ---> error
d='sarab'
e="sarab"
print(d)
print("value of d {}".format(d))
print(e)
print("value of e {}".format(e))



